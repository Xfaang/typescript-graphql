  "peerDependencies": {
    "graphql": "^14.7.0 || ^15.3.0",
    "typescript": ">4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@types/yargs": "^15.0.10",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
